{
    "name": "s3huber/tinkeround",
    "description": "Enhancing Laravel Tinker for tinkering a( )round.",
    "type": "library",
    "keywords": ["laravel", "tinker", "devtools"],
    "homepage": "https://github.com/s3huber/tinkeround",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Huber",
            "homepage": "https://hejfish.com"
        }
    ],
    "require": {
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "3.4.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": { "Tinkeround\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tinkeround\\Tests\\": "tests/" }
    },
    "config": {
        "sort-packages": true
    }
}
